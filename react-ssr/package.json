{
  "name": "react-ssr",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^1.1.5",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "prettier --check --write . && eslint src",
    "testci": "cross-env CI=true react-scripts test --env=jsdom",
    "test": "react-scripts test --env=jsdom",
    "testc": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "command": "npm run lint && git add -A . && npm run testci -- -u"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^26.6.0",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "husky": "^7.0.2",
    "prettier": "^2.4.0",
    "prop-types": "^15.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm command",
      "post-commit": "echo \"[Husky] applying post commit\"",
      "pre-push": "npm run command"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  }
}
